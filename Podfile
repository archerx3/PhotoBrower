# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'PhotoBrower' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Need frameworks: 'MobileCoreServices', 'ImageIO', 'UIKit', 'QuartzCore'

  pod 'Kingfisher'
  # Loading spinner
  pod 'DRPLoadingSpinner'

  pod 'FLAnimatedImage'

end
